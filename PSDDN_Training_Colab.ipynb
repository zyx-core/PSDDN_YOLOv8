                "os.makedirs('data/shanghaitech', exist_ok=True)\n",
                "with open('data/shanghaitech/colab_train.yaml', 'w') as f:\n",
                "    f.write(yaml_content)\n",
                "print('‚úÖ Config created!')"
            ]
        },
